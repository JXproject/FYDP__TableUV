clc;
close all;
clear all; 


data = importdata('ToFData.txt');
topLeft_X             = data(:,1);
topLeft_Y             = data(:,2);
botRight_X            = data(:,3);
botRight_Y            = data(:,4);
RangeStatus           = data(:,5);
RangeMilliMeter       = data(:,6);
SignalRateRtnMegaCps  = data(:,7);
AmbientRateRtnMegaCps = data(:,8);
EffectiveSpadRtnCount = data(:,9);

x = rand(1,10);
y = rand(1,10);
z = duration(rand(10,1),randi(60,10,1),randi(60,10,1));

plot3(x,y,z,'o','DurationTickFormat','mm:ss')
xlabel('X')
ylabel('Y')
zlabel('Duration')
grid on



